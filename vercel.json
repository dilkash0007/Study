{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    },
    {
      "src": "/(.+)",
      "dest": "/dist/public/index.html"
    },
    {
      "src": "/",
      "dest": "/dist/public/index.html"
    }
  ]
}
